<html>
  <head>
    <title>QPS</title>
    <link rel="apple-touch-icon" sizes="57x57" href="/assets/favicon/gas-station.png">
    <link rel="apple-touch-icon" sizes="60x60" href="/assets/favicon/gas-station.png">
    <link rel="apple-touch-icon" sizes="72x72" href="/assets/favicon/gas-station.png">
    <link rel="apple-touch-icon" sizes="76x76" href="/assets/favicon/gas-station.png">
    <link rel="apple-touch-icon" sizes="114x114" href="/assets/favicon/gas-station.png">
    <link rel="apple-touch-icon" sizes="120x120" href="/assets/favicon/gas-station.png">
    <link rel="apple-touch-icon" sizes="144x144" href="/assets/favicon/gas-station.png">
    <link rel="apple-touch-icon" sizes="152x152" href="/assets/favicon/gas-station.png">
    <link rel="apple-touch-icon" sizes="180x180" href="/assets/favicon/gas-station.png">
    <link rel="icon" type="image/png" sizes="192x192" href="/assets/favicon/android-icon-192x192.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/assets/favicon/gas-station.png">
    <link rel="icon" type="image/png" sizes="96x96" href="/assets/favicon/gas-station.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/assets/favicon/gas-station.png">
    <link rel="manifest" href="/assets/favicon/manifest.json">
    <meta name="msapplication-TileColor" content="#ffffff">
    <meta name="msapplication-TileImage" content="assets/favicon/ms-icon-144x144.png">
    <meta name="theme-color" content="#ffffff">
    <!-- Vendors styles-->
    <link rel="stylesheet" href="/vendors/simplebar/css/simplebar.css">
    <link rel="stylesheet" href="/css/vendors/simplebar.css">
    <!-- Main styles for this application-->
    <link href="/css/style.css" rel="stylesheet">
    <!-- We use those styles to show code examples, you should remove them in your application.-->
    <link href="/css/examples.css" rel="stylesheet">
  </head>

  <body>
    <div class="sidebar sidebar-dark sidebar-fixed" id="sidebar">
      <div class="sidebar-brand d-none d-md-flex">
        <svg class="sidebar-brand-full" width="118" height="46" alt="CoreUI Logo">
          <use xlink:href="/assets/brand/coreui.svg"></use>
        </svg>
      </div>
      <ul class="sidebar-nav" data-coreui="navigation" data-simplebar="">
        <li class="nav-title">MENU</li>

        <li class="nav-item">
          <a class="nav-link" href="/public">
            <svg class="nav-icon">
              <use xlink:href="/vendors/@coreui/icons/svg/free.svg#cil-speedometer"></use>
            </svg> Dashboard
          </a>
        </li>

        <li class="nav-item">
          <a class="nav-link" href="/contactUs">
            <svg class="nav-icon">
              <use xlink:href="/vendors/@coreui/icons/svg/free.svg#cil-drop"></use>
            </svg>Contact Us
          </a>
        </li>
    </div>

    <div class="wrapper d-flex flex-column min-vh-100 bg-light">
      <header class="header header-sticky mb-4">
        <div class="container-fluid">
          <button class="header-toggler px-md-0 me-md-3" type="button" onclick="coreui.Sidebar.getInstance(document.querySelector('#sidebar')).toggle()">
            <svg class="icon icon-lg">
              <use xlink:href="/vendors/@coreui/icons/svg/free.svg#cil-menu"></use>
            </svg>
          </button>

          <a class="header-brand d-md-none" href="#">
            <svg width="118" height="46" alt="CoreUI Logo">
              <use xlink:href="/assets/brand/coreui.svg#full"></use>
            </svg>
          </a>
            
          <nav aria-label="breadcrumb">
            <ol class="breadcrumb my-0 ms-2">
              <li class="breadcrumb-item active"><span>WELCOME TO QATAR PETROL STATIONS</span></li>
            </ol>
          </nav>
          
          <ul class="header-nav ms-3">
            <a class="dropdown-item" href="/login">
              <svg class="icon me-2">
                <use xlink:href="/vendors/@coreui/icons/svg/free.svg#cil-account-logout"></use>
              </svg> Login
            </a>
          </ul>
        </div>
      </header>

      <div class="body flex-grow-1 px-3">
        <div class="container-lg">
            <main>
                {{{body}}}
            </main>
        </div>
      </div>
    </div>
    <!-- CoreUI and necessary plugins-->
    <script src="/vendors/@coreui/coreui/js/coreui.bundle.min.js"></script>
    <script src="/vendors/simplebar/js/simplebar.min.js"></script>
    <script>
        document.addEventListener('DOMContentLoaded', function () {
            // Initialize CoreUI components
            new coreui.CoreUI();
        });
    </script>
  </body>
</html>